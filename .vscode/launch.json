{
"version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "attach",
        "name": "Node: Nodemon",
        "processId": "${command:PickProcess}",
        "restart": true,
        "protocol": "inspector"
      },
      {
      "type": "pwa-chrome",
      "request": "launch",
      "name": "Launch Chrome React localhost",
      "url": "http://localhost:3000/",
      "webRoot": "${workspaceFolder}"
    }
    ]
}